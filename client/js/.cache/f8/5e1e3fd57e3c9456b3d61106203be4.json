},{"source":"src/components/nav.vue","name":null,"original":{"line":63,"column":0},"generated":{"line":133,"column":0}}],"sources":{"src/components/nav.vue":"<template>\n  <div>\n    <div class=\"header\">\n      <nav class=\"navbar navbar-expand-lg navbar-body fixed-top\">\n        <!-- Navbar brand -->\n        <a class=\"navbar-brand js-scroll-trigger logo\" href=\"#\" @click.prevent=\"isMyPost=false; isLoginForm=false\" >\n          <img\n            src=\"https://img.icons8.com/ios/50/000000/medium-logo-filled.png\"\n            \n          />MiniWordpress\n        </a>\n        <!-- Collapsible content -->\n        <div class=\"collapse navbar-collapse\" id=\"basicExampleNav\">\n          <!-- Links -->\n          <ul class=\"navbar-nav col-lg-11\">\n            <!-- <li class=\"nav-item\">\n          <a class=\"nav-link\" href=\"#\">Logout</a>\n            </li>-->\n\n            <!-- Dropdown -->\n            <li class=\"nav-item dropdown\" v-if=\"isLogin\">\n              <a\n                class=\"nav-link dropdown-toggle\"\n                id=\"navbarDropdownMenuLink\"\n                data-toggle=\"dropdown\"\n                aria-haspopup=\"true\"\n                aria-expanded=\"false\"\n              >{{name}}</a>\n              <div class=\"dropdown-menu dropdown-primary\" aria-labelledby=\"navbarDropdownMenuLink\">\n                <a class=\"dropdown-item\" href=\"#\" @click=\"isMyPost = true\">My Dashboard</a>\n                <a class=\"dropdown-item\" href=\"#\" @click=\"logout\">Logout</a>\n              </div>\n            </li>\n          </ul>\n          <!-- Links -->\n          <div class=\"md-form my-0 pr-3\" v-if=\"!isLogin\">\n            \n            <div @click=\"isLoginForm=true; isMyPost=false\">\n              <b>Sign In</b>\n            </div>\n          </div>\n          <!-- Collapsible content -->\n        </div>\n      </nav>\n\n      <home v-if=\"!isMyPost && !isLoginForm\"/>\n    </div>\n    <div v-if=\"isMyPost && isLogin\">\n    <myArticle />\n    </div>\n  \n    <loginform v-if=\"isLoginForm\" @login-type=\"checkLogin\" />\n\n  </div>\n  <!--/header-->\n</template>\n\n<script>\nimport loginform from \"./loginform\";\nimport home from \"./home\";\nimport myArticle from \"./myArticles\";\n\nexport default {\n  components: {\n    loginform,\n    home,\n    myArticle\n  },\n  data() {\n    return {\n      isLoginForm: false,\n      isLogin: false,\n      isMyPost: false,\n    };\n  },\n  computed: {\n    name() {\n      return localStorage.getItem(\"username\");\n    }\n  },\n  created() {\n    if (localStorage.token) {\n      this.isLogin = true;\n    } else {\n      this.isLogin = false;\n    }\n  },\n  methods: {\n    checkLogin(payload) {\n      this.isLogin = payload;\n      this.isLoginForm = false;\n    },\n    logout() {\n      Swal.fire({\n        title: \"Gotta go?\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes !\"\n      }).then(result => {\n        if (result.value) {\n          console.log(\"signout\");\n          // var auth2 = gapi.auth2.getAuthInstance();\n          localStorage.clear();\n          // auth2.signOut().then(function() {\n          //   console.log(\"User signed out.\");\n          // });\n          this.isLogin = false;\n          this.isMyPost = false;\n          Swal.mixin({\n            title: \"Logged out successfully\",\n            toast: true,\n            position: \"center\",\n            showConfirmButton: false,\n            timer: 3000\n          });\n        }\n      });\n    }\n  }\n};\n</script>\n<style>\nbody {\n  background-color: rgb(59, 58, 58);\n}\n\n.navbar-body {\n  background-color: white;\n  margin-bottom: 2rem;\n}\n.header {\n  position: relative;\n  margin-top: 6rem;\n}\n.logo {\n  font-family: \"Varela Round\";\n  font-size: 1.5rem;\n  font-weight: 700;\n  color: black;\n  padding-left: 1.5rem;\n}\n\n.nav-link:hover {\n  background: #5cf0bc;\n  color: black;\n}\n.dropdown-item:hover {\n  background: #5cf0bc;\n  color: black;\n}\n\n.nav-item a {\n  color: black;\n  align-items: center;\n}\n\n#foot{\n  background-color: #5cf0bc;\n  position: relative\n}\n</style>\n"},"lineCount":null}},"error":null,"hash":"380df291e42c3f260dcd98b9c82aa76f","cacheData":{"env":{}}}