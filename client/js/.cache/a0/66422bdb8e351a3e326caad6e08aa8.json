al.mixin({\n  toast: true,\n  position: \"center\",\n  showConfirmButton: false,\n  timer: 3000\n});\n\nexport default {\n  components: {\n    addingArticles,\n    eachArticle\n  },\n  data() {\n    return {\n      articles: []\n    };\n  },\n  created() {\n    axios({\n      method: \"GET\",\n      url: `${baseUrl}/articles`,\n      headers: {\n        token: localStorage.getItem(\"token\")\n      }\n    })\n      .then(({ data }) => {\n        this.articles = data;\n      })\n      .catch(err => {\n        console.log(err);\n      });\n  },\n  methods: {\n    addNewArticle(newdata) {\n      this.articles.push(newdata);\n    },\n    deleteArticle(payload) {\n      let index = this.articles.indexOf(payload);\n      this.articles.splice(index, 1);\n    }\n  }\n};\n</script>\n\n<style>\n.sidebar {\n  color: aliceblue;\n  position: relative;\n}\n\n.card-body {\n  background-color: rgba(255, 255, 255, 0.932);\n  text-align: left;\n}\n\n.card img {\n  padding-top: 1rem;\n  padding-bottom: 2rem;\n  width: 46.3rem;\n}\n\n.card span {\n  color: #49ecb3;\n}\n\n.growup {\n  padding-left: 43rem;\n  text-align: right;\n  font-size: 1.5rem;\n}\n\n.delete,\n.edit {\n  padding-top: 3rem;\n}\n</style>"},"lineCount":null}},"error":null,"hash":"2f148edff4d784fc69828c41714be05c","cacheData":{"env":{}}}